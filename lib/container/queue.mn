load container.linkedlist

class Queue {
	def new(self) {
		self.values = LinkedList()
		return self
	}

	def push(self, value) {
		self.values.append(value)
	}

	def pop(self) {
		value = self.values.first()
		self.values.removeFirst()
		return value
	}

	def top(self) {
		return self.values.first()
	}

	def isEmpty(self) {
		return self.values.isEmpty()
	}

	- values = null
}
